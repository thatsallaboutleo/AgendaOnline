@model List<Contato>

<table id="tbnContatosUsuario" class="display" style="width: 100%">
    <thead>
        <tr>
            <th scope="col" hidden="hidden">#</th>
            <th scope="col">Nome</th>
            <th scope="col">E-mail</th>
            <th scope="col">Telefone</th>
        </tr>
    </thead>
    <tbody>

        @if (Model != null && Model.Any())
        {
            foreach (var contato in Model)
            {
                <tr>
                    <th scope="row">@contato.Id</th>
                    <th>@contato.Nome</th>
                    <th>@contato.Email</th>
                    <th>@contato.Telefone</th>
                </tr>
            }
        }
        else
        {
            <tr class="odd">
                <td valign="top" colspan="4">Nenhum contato encontrado!</td>
            </tr>
        }

    </tbody>
</table>